<h1>Eventos</h1>
{{dump()}}
{% if rows %}
  <ul>
    {% for row in rows %}
      <li>
        <h2>{{ row.title }}</h2>
        <p>{{ row.field_event_date }}</p>
        <p>{{ row.field_event_location }}</p>
        <p>{{ row.body }}</p>
      </li>
    {% endfor %}
  </ul>
{% else %}
  <p>No events found.</p>
{% endif %}
